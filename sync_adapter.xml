<?xml version="1.0" encoding="UTF-8" ?>
<node name="/org/tizen/sync/adapter">
	<interface name="org.tizen.sync.adapter">
	<method name="send_result">
		<arg type="s" name="command_line" direction="in" />
		<arg type="i" name="sync_result" direction="in" />
		<arg type="s" name="sync_job_name" direction="in" />
	</method>
	<signal name="start_sync">
		<arg type="i" name="account_id" direction="in" />
		<arg type="s" name="sync_job_name" direction="in" />
		<arg type="b" name="is_data_change" direction="in" />
		<arg type="a{sv}" name="extra" direction="in" />
	</signal>
	<signal name="cancel_sync">
		<arg type="i" name="account_id" direction="in" />
		<arg type="s" name="sync_job_name" direction="in" />
		<arg type="b" name="is_data_change" direction="in" />
		<arg type="a{sv}" name="extra" direction="in" />
	</signal>
	</interface>
</node>
