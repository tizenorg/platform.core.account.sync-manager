<?xml version="1.0" encoding="UTF-8" ?>
<node name="/org/tizen/sync/manager">
	<interface name="org.tizen.sync.manager">
		<method name="add_sync_job">
			<arg type="b" name="account_less_sync" direction="in" />
			<arg type="i" name="account_id" direction="in" />
			<arg type="a{sv}" name="extra" direction="in" />
			<arg type="s" name="capability" direction="in" />
			<arg type="s" name="app_id" direction="in" />
		</method>
		<method name="remove_sync_job">
			<arg type="b" name="account_less_sync" direction="in" />
			<arg type="i" name="account_id" direction="in" />
			<arg type="s" name="capability" direction="in" />
			<arg type="s" name="app_id" direction="in" />
		</method>
		<method name="add_periodic_sync_job">
			<arg type="b" name="account_less_sync" direction="in" />
