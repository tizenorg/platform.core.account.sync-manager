			<arg type="a{sv}" name="extra" direction="in" />
			<arg type="s" name="capability" direction="in" />
			<arg type="t" name="poll_frequency" direction="in" />
			<arg type="s" name="app_id" direction="in" />
		</method>
		<method name="remove_periodic_sync_job">
			<arg type="b" name="account_less_sync" direction="in" />
			<arg type="i" name="account_id" direction="in" />
			<arg type="s" name="capability" direction="in" />
			<arg type="s" name="app_id" direction="in" />
		</method>
		<method name="add_sync_adapter">
			<arg type="b" name="account_less_sa" direction="in" />
			<arg type="s" name="capability" direction="in" />
			<arg type="s" name="command_line" direction="in" />
		</method>
		<method name="set_sync_status">
			<arg type="b" name="enable" direction="in" />
		</method>
	</interface>
</node>
